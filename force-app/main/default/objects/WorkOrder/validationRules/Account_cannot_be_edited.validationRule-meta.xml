<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_cannot_be_edited</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED(AccountId) 
&amp;&amp;
PRIORVALUE(AccountId) != null
&amp;&amp;
!ISPICKVAL(HOT_Request__r.Status__c, &quot;Annullert&quot;)
&amp;&amp;
!ISPICKVAL(HOT_Request__r.Status__c, &quot;Åpen&quot;)
&amp;&amp;
!$Permission.Validation_Override</errorConditionFormula>
    <errorDisplayField>AccountId</errorDisplayField>
    <errorMessage>Konto kan ikke endres når status på forespørsel er &quot;Godkjent&quot;.</errorMessage>
</ValidationRule>
