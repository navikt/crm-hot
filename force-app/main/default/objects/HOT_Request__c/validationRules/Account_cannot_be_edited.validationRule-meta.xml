<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_cannot_be_edited</fullName>
    <active>true</active>
    <errorConditionFormula>OR(!ISPICKVAL(PRIORVALUE(Status__c), &quot;Åpen&quot;)
 &amp;&amp; !ISPICKVAL(PRIORVALUE(Status__c), &quot;Annullert&quot;) &amp;&amp; ISCHANGED(Account__c) &amp;&amp; ISCHANGED(Status__c) &amp;&amp; !$Permission.Validation_Override,

!ISPICKVAL(Status__c, &quot;Åpen&quot;)
 &amp;&amp; !ISPICKVAL(Status__c, &quot;Annullert&quot;) &amp;&amp; ISCHANGED(Account__c) &amp;&amp; !ISCHANGED(Status__c) &amp;&amp; !$Permission.Validation_Override)</errorConditionFormula>
    <errorMessage>Konto kan ikke endres når status er &quot;Godkjent&quot;.</errorMessage>
</ValidationRule>
