<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MoreThan24HoursUntilStart__c</fullName>
    <description>Returns true if the StartTime is more than 24 hours than Current time</description>
    <externalId>false</externalId>
    <formula>StartTime__c &gt; ( NOW() + 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>More Than 24 Hours Until Start</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
